{
  "name": "paragon-dcx-exercise",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-localhost": "PUBLIC_URL=/ react-scripts build",
    "cy:run": "cypress run --e2e --browser chrome --spec **/*.feature",
    "cy:open": "cypress open --e2e --browser chrome",
    "cypress:execution-tags": "cypress run --e2e --browser chrome --spec **/*.feature --env tags=@regression"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "keywords": [
      "cypress",
      "cucumber"
    ]
  },
  "author": "Marcin Kapturski <mkapturski@gmail.com>",
  "dependencies": {
    "axios": "^1.2.3",
    "dotenv-cra": "^3.0.2",
    "isomorphic-fetch": "^3.0.0",
    "react": "^18.2.0",
    "react-typed": "^1.2.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cucumber/cucumber": "^8.10.0",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "cypress": "^12.3.0",
    "path": "^0.12.7"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
